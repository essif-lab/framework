{
  "name": "essif-lab",
  "version": "0.0.0",
  "main": "./trrt/dist/Run.js",
  "bin": {
    "trrt": "./trrt/dist/Run.js"
  },
  "private": true,
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' trrt/Run.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" trrt/Run.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && trrt",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "compilerOptions": {
    "inlineSourceMap": true,
    "target": "es2020"
  },
  "dependencies": {
    "@docusaurus-terminology/parser": "^1.3.0",
    "@docusaurus-terminology/term": "^1.0.0",
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@material-ui/core": "^4.11.0",
    "browserify-fs": "^1.0.0",
    "classnames": "^2.2.6",
    "clear": "^0.1.0",
    "download": "^8.0.0",
    "download-file-sync": "^1.0.4",
    "figlet": "^1.5.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "tslog": "^4.4.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/clear": "^0.1.2",
    "@types/download": "^8.0.1",
    "@types/figlet": "^1.5.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "browserify": "^17.0.0",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^29.3.1",
    "parse-md": "^2.0.4",
    "ts-jest": "^29.0.3",
    "tsify": "^5.0.4",
    "typescript": "^4.8.4",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^4.0.0"
  },
  "workspaces": [
    "packages\\a"
  ]
}
